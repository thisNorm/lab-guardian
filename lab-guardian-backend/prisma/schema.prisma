// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // ✅ v5에서는 여기에 url을 적어도 에러가 안 납니다! (가장 편함)
  url      = "file:./dev.db"
}

model EventLog {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  cctvLog   String?
  robotLog  String?
  camId     String
}